#get properties
curl -X GET http://localhost:8086/{application}/{profile}[/{label}]
curl -X GET http://localhost:8086/{application}-{profile}.yml
curl -X GET http://localhost:8086/{label}/{application}-{profile}.yml
curl -X GET http://localhost:8086/{application}-{profile}.properties
curl -X GET http://localhost:8086/{label}/{application}-{profile}.properties